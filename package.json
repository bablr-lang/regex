{
  "name": "@bablr/regex-vm",
  "version": "0.0.0",
  "description": "A fully streaming regular expression engine",
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./warmup": "./lib/warmup.js",
    "./internal/debug": "./lib/internal/debug.js",
    "./internal/engine": "./lib/internal/engine.js",
    "./async": "./lib/async/index.js",
    "./async/warmup": "./lib/async/warmup.js",
    "./async/chunked": "./lib/async/chunked.js"
  },
  "files": [
    "lib/**/*.js"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "mocha test/regex.test.js",
    "verify": "npm run lint && npm run test && codecov",
    "format": "prettier --write '**/*.{js,md}'",
    "lint": "eslint ."
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.md": [
      "prettier -c"
    ]
  },
  "dependencies": {
    "@bablr/agast-helpers": "github:bablr-lang/agast-helpers#06c2deeda4804fcb6a5022cf2db677f8e89eb249",
    "@bablr/coroutine": "github:bablr-lang/coroutine#2342d82713731288034df588f11f540d1b76603d",
    "@bablr/helpers": "github:bablr-lang/bablr-helpers#e26f876f56dc7cc5513d37dabb0ba2f73b5b5605",
    "@iter-tools/imm-stack": "^1.1.0",
    "functional-red-black-tree": "^1.0.1",
    "iter-tools-es": "^7.0.2"
  },
  "devDependencies": {
    "@bablr/boot": "github:bablr-lang/boot#0631fe96c69cfa3b0b5071d89925f1d52aa1ee93",
    "@bablr/eslint-config-base": "github:bablr-lang/eslint-config-base#d834ccc52795d6c3b96ecc6c419960fceed221a6",
    "codecov": "^3.6.1",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.27.5",
    "expect": "^29.6.2",
    "lint-staged": "^10.2.11",
    "mocha": "^10.2.0",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "regex",
    "regexp",
    "regular",
    "iterator",
    "iterable",
    "match",
    "test",
    "engine",
    "capture",
    "stream",
    "streaming",
    "non-backtracking"
  ],
  "repository": "git@github.com:bablr-lang/regex-vm.git",
  "homepage": "https://github.com/bablr-lang/regex-vm",
  "author": "Conrad Buck <conartist6@gmail.com>",
  "license": "MIT"
}
