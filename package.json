{
  "name": "@bablr/regex-vm",
  "version": "0.0.0",
  "description": "A fully streaming regular expression engine",
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./warmup": "./lib/warmup.js",
    "./internal/debug": "./lib/internal/debug.js",
    "./internal/engine": "./lib/internal/engine.js",
    "./async": "./lib/async/index.js",
    "./async/warmup": "./lib/async/warmup.js",
    "./async/chunked": "./lib/async/chunked.js"
  },
  "files": [
    "lib/**/*.js"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "mocha test/regex.test.js",
    "verify": "npm run lint && npm run test && codecov",
    "format": "prettier --write '**/*.{js,md}'",
    "lint": "eslint ."
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.md": [
      "prettier -c"
    ]
  },
  "dependencies": {
    "@bablr/agast-helpers": "github:bablr-lang/agast-helpers#8b57f2971fc91f6dd4e23ec626a77c931c877786",
    "@iter-tools/imm-stack": "^1.1.0",
    "functional-red-black-tree": "^1.0.1",
    "iter-tools-es": "^7.0.2"
  },
  "devDependencies": {
    "@bablr/boot": "github:bablr-lang/boot#64184e83c8ef575d7e9bcf2b5417f585382ce09a",
    "@bablr/eslint-config-base": "github:bablr-lang/eslint-config-base#a49dbe6861a82d96864ee89fbf0ec1844a8a5e8e",
    "codecov": "^3.6.1",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.27.5",
    "expect": "^29.6.2",
    "lint-staged": "^10.2.11",
    "mocha": "^10.2.0",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "regex",
    "regexp",
    "regular",
    "iterator",
    "iterable",
    "match",
    "test",
    "engine",
    "capture",
    "stream",
    "streaming",
    "non-backtracking"
  ],
  "repository": "git@github.com:bablr-lang/regex-vm.git",
  "homepage": "https://github.com/bablr-lang/regex-vm",
  "author": "Conrad Buck <conartist6@gmail.com>",
  "license": "MIT"
}
