{
  "name": "@bablr/regex",
  "version": "0.0.0",
  "description": "A fully streaming regular expression engine",
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./internal/debug": "./lib/internal/debug.js",
    "./internal/engine": "./lib/internal/engine.js",
    "./async": "./lib/async/index.js",
    "./async/chunked": "./lib/async/chunked.js"
  },
  "files": [
    "lib/**/*.js"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "mocha",
    "verify": "npm run lint && npm run test && codecov",
    "format": "prettier --write '**/*.{js,md}'",
    "lint": "eslint ."
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.md": [
      "prettier -c"
    ]
  },
  "dependencies": {
    "@bablr/grammar": "github:bablr-lang/grammar#f0a39a03a03db3f921c88698f65c1a4aa0f735d5",
    "@bablr/miniparser": "github:bablr-lang/miniparser#59b5a6e83ea0beddb243dcb5ea4ed2f20e053d0a",
    "@iter-tools/imm-stack": "^1.1.0",
    "functional-red-black-tree": "^1.0.1",
    "iter-tools-es": "^7.0.2",
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "codecov": "^3.6.1",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.27.5",
    "expect": "^29.6.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "regex",
    "regexp",
    "regular",
    "iterator",
    "iterable",
    "match",
    "test",
    "engine",
    "capture",
    "stream",
    "streaming",
    "non-backtracking"
  ],
  "repository": "git@github.com:bablr-lang/regex.git",
  "author": "Conrad Buck <conartist6@gmail.com>",
  "license": "MIT"
}
